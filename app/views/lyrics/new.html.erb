<h2> Add new lyrics to the song <i><%=@song.title %></i></h2>


<%= form_for @song do |f|%>
    <%= f.label :title%><br>
    <%= f.text_field :title%><br>
    <%= f.label :genre %><br>
    <%= f.text_field :genre %><br>
    <%= f.hidden_field :user_id, value: current_user.id%>
    <%= f.submit%>
    <%end%>
